{
  "name": "@zemenay/chat-client",
  "version": "0.1.0",
  "description": "Lightweight TypeScript client + widget + server handlers for the Zemenay chat API.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs",
      "default": "./dist/server.js"
    },
    "./migrations": {
      "default": "./migrations/"
    }
  },
  "files": ["dist", "migrations", "README.md", "LICENSE"],
  "sideEffects": false,
  "scripts": {
    "build": "node -e \"try{require('fs').rmSync('dist',{recursive:true,force:true})}catch(e){}\" && tsc -p tsconfig.build.json && node scripts/emit-cjs.mjs",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "license": "MIT",
  "author": "Zemenay Team",
  "keywords": [
    "chatbot",
    "ai",
    "sdk",
    "typescript",
    "nextjs",
    "react",
    "supabase",
    "gemini"
  ],
  "engines": { "node": ">=18" },
  "publishConfig": { "access": "public" },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fafiyusuf/Zemenay-hackathon.git"
  },
  "bugs": { "url": "https://github.com/fafiyusuf/Zemenay-hackathon/issues" },
  "homepage": "https://github.com/fafiyusuf/Zemenay-hackathon#readme",
  "peerDependencies": {
    "typescript": ">=5.0.0",
    "react": ">=18",
    "react-dom": ">=18"
  }
}
